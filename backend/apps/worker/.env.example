# Google Cloud Platform
GCP_PROJECT_ID=your-gcp-project-id
PUBSUB_SUBSCRIPTION_ID=video-jobs-subscription
GCS_BUCKET_VIDEOS_RAW=video-enhancer-raw
GCS_BUCKET_VIDEOS_ENHANCED=video-enhancer-enhanced

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/video_enhancer

# Model Configuration
DEVICE=cuda
BATCH_SIZE=4

# Performance Optimizations
ENABLE_TENSORRT=false
TARGET_HEIGHT=720
TARGET_WIDTH=1280
MODEL_NUM_INFERENCE_STEPS=4

# Processing
TEMP_DIR=/tmp/video_processing
FRAME_EXTRACTION_FPS=30
OUTPUT_VIDEO_CRF=18
OUTPUT_VIDEO_PRESET=slow
